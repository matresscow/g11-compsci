public class Liu_Max_Pg21Ex1 {

    /**
     * This program prints out a bingo card, formatted in a grid using System.out.format.
     */
    public static void main(String[] args) {
        //print out a bingo card
        System.out.format("%7s %7s %7s %7s %7s", "B","I","N","G","O\n");
        System.out.format("%7s %7s %7s %7s %7s", "2","20","42","60","64\n");
        System.out.format("%7s %7s %7s %7s %7s", "14","25","32","55","70\n");
        System.out.format("%7s %7s %7s %7s %7s", "5","18","FREE","53","67\n");
        System.out.format("%7s %7s %7s %7s %7s", "12","16","31","46","75\n");
        System.out.format("%7s %7s %7s %7s %7s", "10","22","39","59","71\n");
    }
}
