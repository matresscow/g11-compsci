package test;

import java.util.Scanner;

/**
 * @author Max
 */
public class Test {

    static int smallestValue(int[] tie) {
        int smallest = tie[0];
        for (int i = 0; i < tie.length; i++) {
            if (tie [i] < smallest) {
                smallest = tie[i];
            }
        }
        return smallest;
    }

    public static void main(String args[]) {
        int[] tie = {8, 2, 45, 2, 9, 8, 24, 5, 134, 35, 5, 73, 737, 367, 6, 3};
        System.out.println(smallestValue(tie));
    }
}
