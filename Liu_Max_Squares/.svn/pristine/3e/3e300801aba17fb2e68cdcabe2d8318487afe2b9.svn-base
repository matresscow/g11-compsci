package liu_max_squares;

/**
 *
 * @author Max
 */
public class Liu_Max_Squares {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        //make the array of 5 elements
        int[] tie = new int[5];
        //for each element, set it as its own square, then display it.
        for (int i = 0; i < tie.length; i++) {
            tie[i] = i*i;
            System.out.println(tie[i]);
        }
    }
}
